{
  "name": "green",
  "version": "1.0.0",
  "description": "TouchDesigner WebRTC视频传输应用",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "signaling": "node webrtc_config/signaling-server.js",
    "dev": "concurrently \"npm run signaling\" \"npm run start\"",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.green.app",
    "productName": "Green",
    "directories": {
      "output": "dist"
    },
    "win": {
      "icon": "ui/asset/App_icon.png"
    },
    "mac": {
      "icon": "ui/asset/App_icon.png"
    },
    "linux": {
      "icon": "ui/asset/App_icon.png"
    }
  },
  "keywords": [
    "electron",
    "touchdesigner",
    "webrtc"
  ],
  "author": "mao",
  "license": "MIT",
  "dependencies": {
    "@serialport/parser-readline": "^13.0.0",
    "electron": "^24.3.0",
    "express": "^4.18.2",
    "face-api.js": "^0.22.2",
    "opencv.js": "^1.2.1",
    "p5": "^1.11.3",
    "serialport": "^13.0.0",
    "three": "^0.175.0",
    "uuid": "^11.1.0",
    "webrtc-adapter": "^9.0.1",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "electron-builder": "^26.0.12"
  }
}
